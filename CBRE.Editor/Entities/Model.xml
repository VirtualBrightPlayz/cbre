<entity name="model">
    <rmesh>
        <write property="file" as="B3DString" />
        <write property="position" as="Vector" />
        <write property="angles" as="Vector" />
        <write property="scale" as="Vector" />
    </rmesh>

    <properties>
        <property name="position" type="Position" />
        <property name="file" type="String" />
        <property name="angles" type="Vector" default="0.0 0.0 0.0" />
        <property name="scale" type="Vector" default="1.0 1.0 1.0" />
        <property name="bake" type="Boolean" default="true" />
    </properties>

    <model name="file" />
</entity>
